	<sql id="selectYfgOrderVo">
        select o.id, o.order_id,o.del_flag, o.third_order_id, o.create_date, o.device_id, o.package_id, o.package_name, o.user_id, o.open_id, o.pay_amount, o.update_time, o.start_time, o.expect_end_time, o.end_time, o.consume_amount, o.refound_time, o.device_type, o.status, o.pay_nofity_time, o.refound_nofity_time, o.remark, o.open_door_time,
        u.id,u.open_id,u.login_name,u.nick_name,u.mobile,
        p.use_time,p.use_money
         from yfg_order o
         left join col_user u on o.user_id = u.id
         left join yfg_package p on o.package_id=p.id
    </sql>
	
